{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "baseUrl": "src",
    "paths": {
      "*": [
        "*"
      ]
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}