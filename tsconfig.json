{
  "compilerOptions": {
    "target": "ES5",
    "module": "ESNext",
    "lib": [
      "ESNext"
    ],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "baseUrl": "src",
    "paths": {
      "*": [
        "*"
      ]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}